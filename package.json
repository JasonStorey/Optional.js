{
  "name": "optional-js",
  "version": "1.1.2",
  "description": "Java 8 Optionals for JS",
  "main": "./index.js",
  "scripts": {
    "clean": "mkdir -p ./dist && rm -rf ./dist/*",
    "build": "npm run clean && browserify ./index.js --standalone Optional -o ./dist/optional.js && npm run minify",
    "minify": "uglifyjs ./dist/optional.js > ./dist/optional.min.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JasonStorey/Optional.js.git"
  },
  "author": {
    "name": "Jason A. Storey",
    "url": "https://github.com/JasonStorey"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonStorey/Optional.js/issues"
  },
  "homepage": "https://github.com/JasonStorey/Optional.js",
  "devDependencies": {
    "browserify": "^8.1.3",
    "mocha": "^2.1.0",
    "uglify-js": "^2.4.16"
  },
  "keywords": [
    "java",
    "oracle",
    "optional",
    "functional",
    "null",
    "javascript",
    "lambda",
    "optionals",
    "nullable",
    "ifPresent"
  ]
}
